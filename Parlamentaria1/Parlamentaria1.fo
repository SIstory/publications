                                 end-indent="0mm"
                                 color="black">
                     <fo:list-item>
                        <fo:list-item-label end-indent="label-end()">
                           <fo:block font-size="70%" line-height="0.9em">[186]</fo:block>
                        </fo:list-item-label>
