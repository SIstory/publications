                       border-color="#7f7f7f"
                       border-style="solid"
                       margin-left="2px"
                       padding-left="4px"
                       font-weight="normal">govor.3.18.</fo:inline>
         </fo:block>
            <fo:block font-size="12pt"
                   font-family="Arial"
                   line-height="15pt"
                   space-after.optimum="5pt"
                   text-align="justify"
                   color="#6f6f6f"
