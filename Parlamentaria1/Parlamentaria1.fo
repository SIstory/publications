                                 color="black">
                     <fo:list-item>
                        <fo:list-item-label end-indent="label-end()">
                           <fo:block font-size="70%" line-height="0.9em">[77]</fo:block>
                        </fo:list-item-label>
                        <fo:list-item-body start-indent="body-start()">
                           <fo:block id="ftn77" font-size="0.83em" line-height="1.0em">
                  Poročilo za obdobje 23. 12. '92 do 16. 10. '96. Ljubljana 1996, str. 1.</fo:block>
                        </fo:list-item-body>
                     </fo:list-item>
                  </fo:list-block>
               </fo:footnote-body>
            </fo:footnote>
